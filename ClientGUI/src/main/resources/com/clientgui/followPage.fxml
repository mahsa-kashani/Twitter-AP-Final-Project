<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane prefHeight="614.0" prefWidth="515.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clientgui.FollowPageController">
  <content>
      <TabPane fx:id="tabpane" prefHeight="614.0" prefWidth="515.0" styleClass="tab2" stylesheets="@styles.css" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="followersPane" text="Followers">
               <content>
                  <VBox fx:id="followersVbox" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Button mnemonicParsing="false" text="Button" />
                              <VBox prefHeight="200.0" prefWidth="100.0" />
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                           </children>
                        </HBox>
                     </children></VBox>
               </content>
            </Tab>
          <Tab fx:id="followingPane" text="Following">
               <content>
                  <VBox fx:id="followingVbox" prefHeight="200.0" prefWidth="100.0" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
  </content>
</ScrollPane>
